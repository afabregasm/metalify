<div class="">
    {{#each track}}
    <img src="{{album.images.0.url}}" width="200" height="200" />
    <h5 class="songnametext">{{name}}</h5>
    <p><a href="/artist/{{artists.0.id}}">{{artists.0.name}}</a></p>
    <p><a href="/album/{{album.id}}">{{album.name}}</a></p>
    <figure>
        <audio
        controls
        src="{{preview_url}}">
            Your browser does not support the
            <code>audio</code> element.
        </audio>
    </figure>
    {{/each}}
</div>
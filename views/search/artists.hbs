{{!-- <h2>{{artist.0.name}} Search results:</h2> --}}
<div>
    {{#each artist}}
    <div id="artists" onclick="location.href='/artist/{{id}}'">
        <p><img src="{{images.0.url}}" alt="{{name}}" width="250px" height="250px" style="border-radius: 50%"/></p>
        <h3>{{name}}</h3>
    </div>
    {{/each}}
</div>